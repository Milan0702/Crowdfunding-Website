{
  "version": 2,
  "buildCommand": "cd ecf2 && npm install && npm run build",
  "outputDirectory": "ecf2/build",
  "installCommand": "cd ecf2 && npm install",
  "framework": "create-react-app",
  "builds": [
    {
      "src": "ecf2/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@api_url"
  }
} 