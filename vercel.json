{
  "version": 2,
  "buildCommand": "cd ecf2 && npm install && npm run build",
  "outputDirectory": "ecf2/build",
  "framework": null,
  "builds": [
    {
      "src": "ecf2/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://crowdfunding-website-production.up.railway.app"
  }
} 